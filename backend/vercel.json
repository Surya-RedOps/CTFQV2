{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_URI": "mongodb+srv://user:user@db.urcwnqc.mongodb.net/?appName=db",
    "JWT_SECRET": "pwnGrid-super-secure-jwt-secret-key-production-2024-minimum-32-chars",
    "SESSION_SECRET": "pwnGrid-super-secure-session-secret-key-production-2024-minimum-32-chars",
    "CORS_ORIGIN": "*"
  }
}